@model library.Models.Student
@await Component.InvokeAsync("AboutNavbar")
@await Component.InvokeAsync("ChildSubNavbar")

<div class="student_wrapper">
    <form asp-controller="Students" asp-action="Create" method="post">
        <div class="form-group">
            <label asp-for="EnrollmentDate" class="control-label"></label>
            <div>
                <input asp-for="EnrollmentDate" style="color:black;" />
                <span asp-validation-for="EnrollmentDate" class="text-danger"></span>
            </div>
        </div> 
        <div class="form-group">
            <label asp-for="FirstMidName" class="control-label"></label>
            <div>
                <input asp-for="FirstMidName"  style="color:black;" />
                <span asp-validation-for="FirstMidName" class="text-danger"></span>
            </div>
        </div>
        <div class="form-group">
            <label asp-for="LastName" class="control-label"></label>
            <div>
                <input asp-for="LastName"  style="color:black;" />
                <span asp-validation-for="LastName" class="text-danger"></span>
            </div>
        </div>
        <div class="form-group"> 
            <div class="">
                <button type="submit" class="btn btn-secondary btn-lg active" role="button" aria-pressed="true">Create</button>
            </div>
        </div>
    </form>
</div>